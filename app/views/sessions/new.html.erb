<h1>Sign In</h1>
<h5>Don't have an account? <%= link_to "Sign Up!", new_user_path %></h5>

<%= form_with(url: session_path) do |f| %>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>

  <p><%= f.submit "Login" %></p>
<% end %>